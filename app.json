{
  "expo": {
    "name": "Smart Controller",
    "slug": "smart-controller",
    "backgroundColor": "#dde0e0",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#bbc7c9",
    "icon": "./assets/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#dde0e0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.htanjo.smartcontroller",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_fg.png",
        "backgroundImage": "./assets/icon_bg.png"
      },
        "permissions": [
        "VIBRATE"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000"
    }
  }
}
